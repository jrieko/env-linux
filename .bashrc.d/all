#!/usr/bin/env bash
debug "file: ${BASH_SOURCE[0]}"

alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias vim="vim -o"
alias less="less -R"
#alias cp="rsync -avh --progress"

alias vim.bash=". ~/bin/.vim.bash"
alias vim.which=".which vim"
alias l.which=".which l"

alias tail.latest="set -f && . ~/bin/.latest 'tail -f'"

